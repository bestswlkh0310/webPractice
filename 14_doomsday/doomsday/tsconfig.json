{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES5",
        "lib": [
            "ES6",
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "moduleResolution": "node",
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
    },
    "include": [
        "./src"
    ]
}
